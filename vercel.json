{
    "version":2,
    "builds":[
        {
            "src":"drf/wsge.py",
            "use": "@vercel/vercel-python-wsgi",
            "config":{
                "maxLamdaSize":"15mb",
                "runtime":"python3.9"

            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"/drf/wsgi.py"
        }
    ]
}