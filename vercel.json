{
    "version":2,
    "builds":[
        {
            "src":"drf/wsgi.py",
            "use": "@vercel/python",
            "config":{
                "maxLamdaSize":"15mb"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"drf/wsgi.py"
        }
    ]
}